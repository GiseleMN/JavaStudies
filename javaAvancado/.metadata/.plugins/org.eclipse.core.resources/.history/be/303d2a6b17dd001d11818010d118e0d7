package br.com.newMyProject;

public class ContaTeste {
	 public static void main(String[]args) {
		 Conta conta = new Conta();
		 conta.Cliente = "Joana";
		 conta.saldo = 4000.00;
		 conta.exibeSaldo();
		 
		 System.out.println("Voce fez um saque de:" + conta.saque(1200.00) );
		 
		 ;
		 conta.exibeSaldo();
		 
		 conta.deposito(3000.00);
		 conta.exibeSaldo();
	 }
	
}

